{
    "kind": "collectionType",
    "collectionName": "eventos",
    "info": {
        "singularName": "evento",
        "pluralName": "eventos",
        "displayName": "Evento"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "titulo": {
            "type": "string"
        },
        "subtitulo": {
            "type": "text"
        },
        "descricao": {
            "type": "text"
        },
        "categoria": {
            "type": "string"
        },
        "notacao": {
            "type": "string"
        },
        "data": {
            "type": "date"
        },
        "horas": {
            "type": "time"
        },
        "imagem": {
            "type": "media",
            "multiple": false,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "like": {
            "type": "integer"
        },
        "slug": {
            "type": "uid"
        },
        "local": {
            "type": "string"
        },
        "capacidade": {
            "type": "integer"
        },
        "preco": {
            "type": "decimal"
        },
        "bloco": {
            "type": "string"
        },
        "contato": {
            "type": "text"
        },
        "comentarios": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::comentario.comentario",
            "mappedBy": "evento"
        }
    }
